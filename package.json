{
  "name": "@11thdeg/cyan-next",
  "private": false,
  "version": "1.0.0-alpha.29",
  "description": "A lightweight web component library for Pelilauta, Cinis etc. projects",
  "main": "./dist/cyan.umd.cjs",
  "module": "./dist/cyan.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
